{
    "name": "vscode-wechat",
    "displayName": "vscode-wechat",
    "description": "wechat app support for vscode",
    "version": "0.0.1",
    "publisher": "qinjia",
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/hellopao/vscode-wechat.git"
    },
    "homepage": "https://github.com/hellopao/vscode-wechat",
    "bugs": {
        "url": "https://github.com/hellopao/vscode-wechat/issues"
    },
    "activationEvents": [
        "workspaceContains:app.json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}
