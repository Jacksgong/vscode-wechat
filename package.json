{
  "name": "vscode-wechat",
  "displayName": "vscode-wechat",
  "description": "wechat app support for vscode",
  "version": "0.0.4",
  "publisher": "qinjia",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hellopao/vscode-wechat.git"
  },
  "homepage": "https://github.com/hellopao/vscode-wechat",
  "bugs": {
    "url": "https://github.com/hellopao/vscode-wechat/issues"
  },
  "activationEvents": [
    "workspaceContains:app.json"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.previewWechatApp",
        "title": "Preview Wechat App"
      }
    ],
    "keybindings": [
      {
        "command": "extension.previewWechatApp",
        "key": "ctrl+f8",
        "mac": "cmd+f8",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.46",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "detect-port": "^1.0.5",
    "download": "^5.0.2",
    "portfinder": "^1.0.9",
    "wept": "^0.5.4"
  }
}
